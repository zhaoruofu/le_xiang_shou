
<view class='container'>
    <image class='img' src='images/announcement.png'></image>

    <swiper class="swiper_container" vertical="true" autoplay="true" circular="true" interval="1000">
        <block wx:for="{{msgList}}" wx:key='u'>
                <swiper-item>
                    <view class="swiper_item">{{item}}</view>
                </swiper-item>
        </block>
    </swiper>
    <view bind:tap='onTap' class='frame'>
        <text class='detail'>详情</text>
    </view>
</view>

