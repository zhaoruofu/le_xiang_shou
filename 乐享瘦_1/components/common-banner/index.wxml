
<swiper class="swiper_container" 
    autoplay="true" 
    circular="true"
    interval="2000"
    indicator-dots="true"
    indicator-color="white"
 >
    <block wx:for="{{imgUrls}}" wx:key='u'>
         <swiper-item>
            <image class='imgUrls' src='{{item}}'></image>
         </swiper-item>
    </block>
</swiper>